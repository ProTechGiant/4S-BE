{
  "name": "4s-project",
  "version": "1.0.0",
  "description": "RESTful API 4s with Typescript, TypeORM, ExpressJs & Typeorm",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run server:start",
    "server:start": "npx ts-node-dev --respawn --transpile-only src/server.ts",
    "socket:start": "npx ts-node-dev --respawn --transpile-only src/socket.server.ts",
    "test:dev": "cross-env NODE_ENV=development IS_TEST=true jest",
    "start": "cross-env NODE_ENV=production npx ts-node-dev --respawn --transpile-only src/server.ts",
    "test:prod": "cross-env NODE_ENV=production IS_TEST=true jest --detectOpenHandles",
    "commit": "git-cz",
    "build": "rimraf dist && tsc",
    "swagger": "npx ts-node src/swagger/swagger.output.ts",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc --noEmit",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "test:coverage": "cross-env IS_TEST=true jest --coverage",
    "docker:dev": "docker-compose --file docker-compose.yml --file docker-compose.dev.yml --compatibility up --build",
    "docker:stage": "docker-compose --file docker-compose.yml --file docker-compose.stage.yml --compatibility up --build",
    "docker:prod": "docker-compose --file docker-compose.yml --file docker-compose.prod.yml --compatibility up --build",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f src/database/typeorm.config-migrations.ts",
    "migration:generate": "npm run typeorm:cli -- migration:generate -d src/database/migrations -n",
    "migration:create": "npm run typeorm:cli -- migration:create -d src/database/migrations -n",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "seeder:run": "cross-env MR=seeder npm run typeorm:cli -- migration:run"
  },
  "dependencies": {
    "@types/jest": "^29.5.3",
    "@types/socket.io": "^3.0.2",
    "@types/websocket": "^1.0.5",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "csv-parse": "^5.4.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "graphology": "^0.25.4",
    "graphology-shortest-path": "^2.0.2",
    "graphology-types": "^0.24.7",
    "helmet": "^5.0.2",
    "image-size": "^1.0.2",
    "ioredis": "4.28.3",
    "jest": "^29.6.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mqtt": "^4.3.7",
    "multer": "^1.4.5-lts.1",
    "nodenv": "^0.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^29.1.1",
    "twilio": "^4.19.3",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.0.0",
    "validator": "^13.7.0",
    "websocket": "^1.0.34",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.1",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.58",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "swagger-autogen": "^2.23.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.6.4"
  }
}
